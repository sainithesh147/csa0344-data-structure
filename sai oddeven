#include<stdio.h>
int main()
{
int a[20],i,n;
printf("Enter no.of.elements:");
scanf("%d",&n);
printf("Enter elements:");
for(i=0;i<n;i++)
{
scanf("%d",&a[i]);
}
printf("Even numbers in an array:\n");

for(i=0;i<n;i++)
{
if (a[i]%2==0)

   printf("%d ",a[i]);
}
printf("Odd numbers in an array:\n");
for(i=0;i<n;i++)
{
if(a[i]%2!=0)
 
  printf("%d ",a[i]);
}
}


