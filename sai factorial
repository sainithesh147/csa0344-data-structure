#include<stdio.h>
int main()
{
int i,n,fact=1;
printf("Enter a number:");
scanf("%d",&n);
for(i=1;i<=n;i++)
{
fact=fact*i;
}
printf("Factorial of a given number :%d\n",fact);
}

output:
user@user-HP-ProDesk-400-G1-SFF:~$ cc foctorial.c
user@user-HP-ProDesk-400-G1-SFF:~$ ./a.out
Enter a number:4   
Factorial of a given number :24

