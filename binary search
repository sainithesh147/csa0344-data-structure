// sai nithesh 192210574
#include<stdio.h>
int main()
{
int i,n,a[50],key,flag=0,first,last,mid;
printf("Enter n:\n");
scanf("%d",&n);
printf("Enter elements i a sorted order:");
for(i=0;i<n;i++)
{
scanf("%d",&a[i]);
}
printf("Enter key element to search:\n");
scanf("%d",&key);
first=0;
last=n-1;
while(first<=last)
{
mid=(first+last)/2;
if(a[mid]==key)
{
flag=1;
break;
}
else if(a[mid]<key)
first=mid+1;
else if(a[mid]>key)
last=mid-1;
}
if(flag==1)
printf("The key element found at %d",mid+1 );
else 
printf("The key element is not found");
}
